
package Main {

    main fn test(arr String){
        withreg r {
            val foo = new@r Foo(42);
            val bar = new@r Bar(foo);
            doSomething(foo, bar);
        }
    }

    fn doSomething(f: Foo^, b: Bar^){
        console.print((f.i == b.f.i) as String);
    }

}

mut struct Foo {
    var i: Int
}

mut struct Bar {
    var f: Foo^{reg}
}
