
package Main {

    main fn main_(arr String) {
        val text: String;
        enclosed {fs} {
            text = Untrusted.readFile("../iores/data.txt");
        };
        var line = 1;
        var lineStart = true;
        for var ci = 0; ci < len text; ci += 1 {
            if lineStart {
                print("Line " + intToString(line) + ": ");
                lineStart = false;
            };
            print(charToString(text[ci]));
            if text[ci] == '\n' {
                lineStart = true;
                line += 1;
            }
        }
    }

}
