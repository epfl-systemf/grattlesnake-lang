
package Test {

    fn createArray() -> arr Double {
        return [f(10.0), 14.31, -2.1, 11.0+9.4, -5.0*28.0, 94.35*32.21]
    }

    fn f(x: Double) -> Double {
        val y = 2.0*x - 1.0;
        return y * y
    }

    fn printArray(xs: arr Double){
        for var i = 0; i < #xs; i += 1 {
            print(doubleToString(xs[i]) + " ");
        }
    }

    main fn test(arr String){
        printArray(createArray());
    }

}
