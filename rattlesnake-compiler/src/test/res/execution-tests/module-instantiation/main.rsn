
mut struct Foo {
    var i: Int
}

module Bar(rg: Region^, foo: Foo^{rg}){

    fn getFoo() -> Foo^{me.rg} {
        return me.foo;
    }

}

package Main {

    main fn program(arr String){
        val r = newregion;
        val bar = new Bar(r, new@r Foo(99));
        val foo: Foo^{r} = bar.getFoo();
        print(intToString(foo.i));
    }

}
