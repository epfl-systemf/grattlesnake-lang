#nocap;

package Test {

    main fn testFunc(arr String) {
        val array = [12, 25, 33, 41, 28, 22, 27, 91, 0, -7, 14, -5, 9];
        val counter = new Counter(0, -1);
        counter.odds = 0;
        for var k = 0; k < #array; k += 1 {
            me.add(counter, array[k]);
        };
        print(Counters.counterToString(counter));
    }
    
    fn add(counter: Counter, n: Int){
        if n % 2 == 0 {
            counter.evens += 1;
        } else {
            counter.odds += 1;
        }
    }

}

mut struct Counter {
    var evens: Int,
    var odds: Int
}

package Counters {

    fn counterToString(counter: Counter) -> String {
        return intToString(counter.evens) + " evens and " + intToString(counter.odds) + " odds";
    }

}
