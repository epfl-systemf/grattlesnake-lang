
const success = "OK";
const failure: String = "K.O.";

package Test {

    fn testFunc(xarr: arr Int) -> String {
        var x = xarr[0];
        x += neg;
        x -= (neg/(-neg2));
        if x < ref {
            return success;
        } else {
            return failure;
        }
    }
    
    main fn test(arr String){
        print(testFunc([41]) + "\n");
        print(testFunc([807]) + "\n");
        print(testFunc([553]) + "\n");
        print(testFunc([552]) + "\n");
    }

}

const ref = 544

const neg = -12
const neg2 = -4
